			Qpo_Vec3d_Inverse(&Q_WindWake[aero].VecUp);
			Qpo_Transform_GetLeft(&Forge[aero].Avatar.ActorXform, &Q_WindWake[aero].VecLeft);
			Q_WindWake[aero].VecIn.x = 0;
			Q_WindWake[aero].VecIn.y = 1;
